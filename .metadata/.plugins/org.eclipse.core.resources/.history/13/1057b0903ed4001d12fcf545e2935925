
public class SwitchExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         String value="Wednesday";
         String result="";
         switch(value) {
         case "Sunday","Saturday":
             result="Holiday";
             break;
         case "Monday","Tuesday","Wednesday":
             result="Working";
             break;
             default:
            	 result="Not a day";
         }
         System.out.println(result);
	}

}
