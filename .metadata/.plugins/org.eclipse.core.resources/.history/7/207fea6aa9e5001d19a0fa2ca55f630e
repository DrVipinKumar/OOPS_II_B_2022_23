package objectcloning;
class Rectangle{
	int l, b;
	
	void getArea() {
		System.out.println("Area ="+(l*b));
	}
	
}

public class CloningEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
          Rectangle r1 =new Rectangle();
          r1.l=20;
          r1.b=10;
          r1.getArea();
          Rectangle r2=r1;
	}

}
