package objectcloning;
class Rectangle {
	int l, b;
	Rectangle(int l, int b){
		this.l=l;
		this.b=b;
	}
	void getArea() {
		System.out.println("Area ="+(l*b));
	}
}

public class CloningEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
           Rectangle r1 =new Rectangle();
           r1.
	}

}
