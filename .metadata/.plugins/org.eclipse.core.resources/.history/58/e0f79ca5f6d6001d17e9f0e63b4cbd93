class Sum {
	private int num1;
	private int num2;
	Sum(int num1, int num2){
		this.num1=num1;
		this.num2=num2;
	}
	int getSum() {
		return num1+num2;
	}
}
public class Calculator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         Sum s1=new Sum(23,45);
         //Constructor is a special method that have same name as name of class
         //It use to initialize variable in class. 
         //It use to create object of a class.
         //It does not return any value. 
         int add=s1.getSum();
         System.out.println("Sum =%d".formatted(add));
         Sum s2=new Sum(56,78);
        add=s2.getSum();
         System.out.println(String.format("Dr. Vipin", "Classes"));
         
	}

}
