package polymorphism;

class A {
	void display() {
		System.out.println("In class A");
	}
}

class B extends A {
	void displayB() {
		System.out.println("In class B");
	}
	
}
public class PolyEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	    B a1 =new B();
		a1.display();

	}

}
