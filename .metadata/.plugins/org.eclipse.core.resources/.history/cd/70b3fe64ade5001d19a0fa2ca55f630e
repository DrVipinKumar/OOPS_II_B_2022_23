package innerclass;
class Outer {
	void display() {
		System.out.println("Outer Class");
	}
	class Inner {
		void display() {
			System.out.println("Inner Class");
		}
	}
	static class Inner1 {
		void display() {
			System.out.println("Inner1 Class");
		}
	}
}

public class InnerClassEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			Outer o1 =new Outer();
			o1.display();
			Outer.Inner in1 =o1.new Inner();
			in1.display();
			Outer.Inner1 in2 =new Outer.Inner1();
			in2.display();
			
			
	}

}
