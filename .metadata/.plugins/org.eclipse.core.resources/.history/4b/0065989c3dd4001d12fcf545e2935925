
public class SwitchExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         String value="Monday";
         switch(value) {
         case "Sunday":
            System.out.println("Holiday");
            break;
         case "Saturday":
             System.out.println("Holiday");
             break;
         case "Monday":
             System.out.println("Working");
             break;
         case "Tuesday":
             System.out.println("Working");
             break;
         case "Wednesday":
             System.out.println("Working");
             break;
             default:
            	 System.out.println("Not a day");
         }
        
	}

}
